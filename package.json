{
  "name": "@evmlord/multicall-sdk",
  "version": "0.0.5-alpha.5",
  "description": "A powerful multichain lightweight library to process multiple calls via a single eth_call using Ethers JS.",
  "scripts": {
    "alpha": "npm publish --tag alpha",
    "gen": "ts-node scripts/gen-multicall.ts",
    "gen:readme": "ts-node scripts/generate-supported-networks.ts",
    "gen:toc": "ts-node scripts/generate-toc.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn test && yarn gen:readme && yarn build",
    "test": "mocha -r ts-node/register \"test/**/*.spec.ts\" --exit",
    "dev:link": "node scripts/copy-files.mjs ../dist/ ../package.json ../README.md ../link-out/ --replace && (cd link-out && yarn link)",
    "watch:link": "tsc -w & nodemon -q -e js,ts --exec \"yarn dev:link\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EVMlord/multicall-sdk.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "multicall",
    "multicall3",
    "batch-calls",
    "ethers",
    "ethers.js",
    "ethereum",
    "multichain",
    "library",
    "sdk"
  ],
  "author": {
    "name": "EVMlord",
    "email": "buidl@evmlord.dev",
    "url": "https://github.com/EVMlord"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EVMlord/multicall-sdk/issues"
  },
  "homepage": "https://github.com/EVMlord/multicall-sdk#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "ethers": "^6.13.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.23",
    "@types/sinon": "^17.0.4",
    "chai": "^5.2.0",
    "ethers": "^6.13.0",
    "mocha": "^11.5.0",
    "sinon": "^20.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
